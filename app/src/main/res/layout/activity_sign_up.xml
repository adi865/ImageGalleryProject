<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    style="@style/groupStylegray_100"
    tools:context=".ui.SignUpActivity">

    <ImageView android:id="@+id/imageImageOne"
        android:layout_gravity="center_horizontal"
        android:layout_height="@dimen/_73pxh"
        android:layout_marginEnd="@dimen/_32pxh"
        android:layout_marginStart="@dimen/_32pxh"
        android:layout_marginTop="@dimen/_16pxv"
        android:layout_width="@dimen/_75pxh"
        android:scaleType="fitXY"
        tools:ignore="ContentDescription"
        android:src="@drawable/img_image1"/>

    <TextView
        android:ellipsize="end"
        android:layout_gravity="center_horizontal"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_32pxh"
        android:layout_marginStart="@dimen/_32pxh"
        android:layout_marginTop="@dimen/_15pxv"
        android:layout_width="wrap_content"
        android:lineSpacingExtra="@dimen/_5pxh"
        android:singleLine="true"
        android:text="Sign Up"
        style="@style/txtIntersemibold20" tools:text="Sign Up"/>

    <LinearLayout android:gravity="start"
        android:id="@+id/linearInputfieldPla"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_23pxh"
        android:layout_marginStart="@dimen/_23pxh"
        android:layout_marginTop="@dimen/_24pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylecornerRadius">

    </LinearLayout>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearInputfieldPlat1"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_23pxh"
        android:layout_marginStart="@dimen/_23pxh"
        android:layout_marginTop="@dimen/_20pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylecornerRadius">
        <TextView android:ellipsize="end"
            android:id="@+id/txtBodyOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_3pxh"
            android:singleLine="true"
            android:text="Your Full Name"
            style="@style/txtInterbold12"/>
        <EditText
            android:id="@+id/displayName"
            android:background="@drawable/rectangle_bg_gray_50_radius_15"
            android:ellipsize="end"
            android:hint="Enter your name"
            android:inputType="textEmailAddress"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_3pxh"
            android:paddingBottom="@dimen/_15pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_20pxh"
            android:paddingTop="@dimen/_17pxv"
            android:singleLine="true"
            android:textColorHint="@color/bluegray_200"
            style="@style/etSolidRounded" tools:ignore="Autofill"/>
    </LinearLayout>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearInputfieldPla1"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_23pxh"
        android:layout_marginStart="@dimen/_23pxh"
        android:layout_marginTop="@dimen/_20pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylecornerRadius">
        <TextView android:ellipsize="end"
            android:id="@+id/txtBodyThree"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_3pxh"
            android:singleLine="true"
            android:text="Email"
            style="@style/txtInterbold12"/>
        <EditText
            android:id="@+id/uName"
            android:background="@drawable/rectangle_bg_gray_50_radius_15"
            android:ellipsize="end"
            android:hint="Enter your email"
            android:inputType="textEmailAddress"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_3pxh"
            android:paddingBottom="@dimen/_15pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_20pxh"
            android:paddingTop="@dimen/_17pxv"
            android:singleLine="true"
            android:textColorHint="@color/bluegray_200"
            style="@style/etSolidRounded" tools:ignore="Autofill"/>
    </LinearLayout>
    <TextView android:ellipsize="end"
        android:id="@+id/txtBodyFive"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_23pxh"
        android:layout_marginStart="@dimen/_23pxh"
        android:layout_marginTop="@dimen/_20pxv"
        android:layout_width="wrap_content"
        android:lineSpacingExtra="@dimen/_3pxh"
        android:singleLine="true"
        android:text="Password"
        style="@style/txtInterbold12"/>
    <EditText
        android:background="@drawable/rectangle_bg_gray_50_radius_15"
        android:drawableEnd="@drawable/img_eye"
        android:drawablePadding="@dimen/_30pxh"
        android:ellipsize="end"
        android:hint="Enter your password"
        android:id="@+id/password"
        android:inputType="textPassword"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_23pxh"
        android:layout_marginStart="@dimen/_23pxh"
        android:layout_marginTop="@dimen/_15pxv"
        android:layout_width="match_parent"
        android:lineSpacingExtra="@dimen/_3pxh"
        android:paddingBottom="@dimen/_18pxv"
        android:paddingEnd="@dimen/_21pxh"
        android:paddingStart="@dimen/_20pxh"
        android:paddingTop="@dimen/_14pxv"
        android:singleLine="true"
        android:textColorHint="@color/bluegray_200"
        style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
    <TextView android:ellipsize="end"
        android:id="@+id/txtBodySix"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_23pxh"
        android:layout_marginStart="@dimen/_23pxh"
        android:layout_marginTop="@dimen/_21pxv"
        android:layout_width="wrap_content"
        android:lineSpacingExtra="@dimen/_3pxh"
        android:singleLine="true"
        android:text="Retype your password"
        style="@style/txtInterbold12"/>
    <EditText
        android:background="@drawable/rectangle_bg_gray_50_radius_15"
        android:drawableEnd="@drawable/img_call"
        android:drawablePadding="@dimen/_30pxh"
        android:ellipsize="end"
        android:hint="Retype your password"
        android:id="@+id/cPassword"
        android:inputType="textPassword"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_23pxh"
        android:layout_marginStart="@dimen/_23pxh"
        android:layout_marginTop="@dimen/_14pxv"
        android:layout_width="match_parent"
        android:lineSpacingExtra="@dimen/_3pxh"
        android:paddingBottom="@dimen/_16pxv"
        android:paddingEnd="@dimen/_21pxh"
        android:paddingStart="@dimen/_20pxh"
        android:paddingTop="@dimen/_16pxv"
        android:singleLine="true"
        android:textColorHint="@color/bluegray_200"
        style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
    <androidx.appcompat.widget.AppCompatButton
        android:ellipsize="end" android:gravity="center"
        android:id="@+id/btnSignUp"
        android:layout_gravity="center_horizontal"
        android:layout_height="@dimen/_50pxv"
        android:layout_marginEnd="@dimen/_23pxh"
        android:layout_marginStart="@dimen/_23pxh"
        android:layout_marginTop="@dimen/_20pxv"
        android:layout_width="@dimen/_184pxh"
        android:lineSpacingExtra="@dimen/_3pxh"
        android:paddingBottom="@dimen/_16pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingStart="@dimen/_30pxh"
        android:paddingTop="@dimen/_16pxv"
        android:singleLine="true"
        android:text="Sign Up"
        android:textAllCaps="false"
        style="@style/btnGradientRounded"/>
    <TextView android:ellipsize="end"
        android:gravity="center_horizontal"
        android:id="@+id/tvSignIn"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/_27pxv"
        android:layout_marginEnd="@dimen/_23pxh"
        android:layout_marginStart="@dimen/_23pxh"
        android:layout_marginTop="@dimen/_16pxv"
        android:layout_width="match_parent"
        android:lineSpacingExtra="@dimen/_3pxh"
        android:singleLine="true"
        android:text="Already have an account? Sign In"
        style="@style/txtRobotoregular12"/>
</LinearLayout>

